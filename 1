
def a():
    for i in range(7):
        ryad = ""
        for j in range(5):
            if i == 0:
                if j == 0:
                    ryad = ryad + " "
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + "*"
                if j == 4:
                    ryad = ryad + " "
            if i == 1 or i == 2 or i == 4 or i == 5 or i == 6:
                if j == 0:
                    ryad = ryad + "*"
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + " "
                if j == 4:
                    ryad = ryad + "*"
            if i == 3:
                if j == 0:
                    ryad = ryad + "*"
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + "*"
                if j == 4:
                    ryad = ryad + "*"

        print(ryad)

def b():
    for i in range(7):
        ryad = ""
        for j in range(5):
            if i == 0:
                if j == 0 or j == 1 or j == 2 or j == 3:
                    ryad = ryad + "*"
            if i == 1:
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + " "
                if j == 0 or j == 4:
                    ryad = ryad + "*"
            if i == 2:
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + " "
                if j == 0 or j == 4:
                    ryad = ryad + "*"
            if i == 3:
                if j == 0 or j == 1 or j == 2 or j == 3:
                    ryad = ryad + "*"
            if i == 4:
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + " "
                if j == 0 or j == 4:
                    ryad = ryad + "*"
            if i == 5:
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + " "
                if j == 0 or j == 4:
                    ryad = ryad + "*"
            if i == 6:
                if j == 0 or j == 1 or j == 2 or j == 3:
                    ryad = ryad + "*"

        print(ryad)

