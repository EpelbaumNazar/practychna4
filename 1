            if i == 2:
                if j == 0 or j == 4:
                    ryad = ryad + "*"
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + " "
            if i == 3:
                if j == 0 or j == 2 or j == 4:
                    ryad = ryad + "*"
                if j == 1 or j == 3:
                    ryad = ryad + " "
            if i == 4:
                if j == 0 or j == 1 or j == 3 or j == 4:
                    ryad = ryad + "*"
                if j == 2:
                    ryad = ryad + " "
            if i == 5:
                if j == 0 or j == 4:
                    ryad = ryad + "*"
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + " "
            if i == 6:
                if j == 0 or j == 4:
                    ryad = ryad + "*"
                if j == 1 or j == 2 or j == 3:
                    ryad = ryad + " "

        print(ryad)


